{
  "ignorePatterns": [
    "*.d.ts",
    "dist"
  ],
	"env": {
		"browser": true,
		"es6": true,
		"node": true,
		"jasmine": true
	},
	"extends": [
    "plugin:@typescript-eslint/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"rules": {}
}